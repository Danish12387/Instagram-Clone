{
    "version": 2,
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/v1/user", "dest": "/api/user.js" },
      { "src": "/api/v1/post", "dest": "/api/post.js" },
      { "src": "/api/v1/message", "dest": "/api/message.js" }
    ]
  }
